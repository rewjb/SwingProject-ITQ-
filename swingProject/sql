create database BBQ;

유통기한..고민..

=가맹점 발주내역=
create table bodyorder{
num int,
name varchar(15),
quantity int,
date varchar(20),
hconfirm varchar(10),
bconfirm varchar(10)
};

=가맹점 매출내역=
create table bodysales{
num int,
id varchar(10) unique,
date varchar(20),
money int,
chickenF int,
chickenH int,
chickenS int,
side int
};

=가맹점 관리=
create table headmember{
id varchar(10) unique not null,
pw varchar(10) not null,
ownername varchar(10) not null,
tel varchar(15) not null,
comnum varchar(15) not null,
addr varchar(50) unique not null,
alias varchar(20) unique not null
};

=가맹점 재고관리=
create table bodystock{
name varchar(15),
quantity int,

};

=가맹점 관리=
create table headmember{
id varchar(10) unique not null,
pw varchar(10) not null,
ownername varchar(10) not null,
tel varchar(15) not null,
comnum varchar(15) not null,
addr varchar(50) unique not null,
alias varchar(20) unique not null
};

=업체 관리=
create table headvender{
id varchar(10) unique not null,
name varchar(10) not null,
date varchar(20),
comnum varchar(15) not null,
tel varchar(15) not null,
};

=제품 관리=
create table headvender{
id varchar(10) unique not null,
name varchar(10) not null,
venderid varchar(10) not null,
money int,
};

